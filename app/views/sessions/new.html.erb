<p id="alert"><%= alert %></p>
<h1>ログイン</h1>
<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %><br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
  <%= f.submit "ログインする" ,class: "btn btn-primary"%>
<% end %>
<%= button_to t('Back'), blogs_path, method: :get%>
<%= button_to 'ユーザ登録', signup_path, method: :get%>

